{
    "name": "izica/laravel-env-secure",
    "description": "Secure Laravel .env variables",
    "keywords": [
        "izica",
        "laravel-env-secure",
        "secure",
        "laravel",
        "laravel env",
        "laravel-env",
        "env"
    ],
    "homepage": "https://github.com/izica/laravel-env-secure",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "izica",
            "email": "artemiztomska@gmail.com",
            "role": "owner"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Izica\\LaravelEnvSecure\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Izica\\EnvSecure\\EnvSecureServiceProvider"
            ],
            "aliases": {
                "EnvSecure": "Izica\\EnvSecure\\EnvSecureFacade"
            }
        }
    }
}
